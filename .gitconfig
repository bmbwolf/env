[user]
	email =
	name =
[color]
        ui = true
[alias]
        co = checkout
        st = status
        di = diff
        ci = commit -a -v
        br = branch
        mt = mergetool
        sta = stash
        lg = log --graph --pretty=format:'%Cred%h%Creset%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset%Cblue(%an)%Creset' --abbrev-commit --date=relative
[core]
	editor = vim
[pull]
-  rebase = true
